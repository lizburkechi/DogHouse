<center><h1> Apply Now:</h1>

<main>
<%= form_for @renter_apartment_application do |f| %>
<p><%= f.label :renter_id %></p>
<p><%= f.collection_select :renter_id, Renter.all, :id, :name %></p>
<br>
<p><%= f.label :apartment_id %>
<p><%= f.collection_select :apartment_id, Apartment.all, :id, :address %></p>
<br>
<p><%= f.label :email %></p>
<p><%= f.text_field :email %></p>
<br>
<p><%= f.submit %></p>

<% end %>

</main>

</center>